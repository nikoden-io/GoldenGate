{
  "name": "golden-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --configuration=development",
    "start:staging": "ng serve --configuration=staging",
    "start:prod": "ng serve --configuration=production",
    "startfr": "ng serve --configuration=fr",
    "start:nl": "ng serve --configuration=nl",
    "build:dev": "ng build --configuration=development",
    "build:staging": "ng build --configuration=staging",
    "build:prod": "ng build --configuration=production",
    "watch:dev": "ng build --watch --configuration development",
    "watch:staging": "ng build --watch --configuration staging",
    "watch:prod": "ng build --watch --configuration production",
    "test": "ng test",
    "storybook": "ng run GoldenFront:storybook",
    "build-storybook": "ng run GoldenFront:build-storybook",
    "extract-i18n": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@ngrx/component-store": "^18.0.2",
    "@ngrx/effects": "^18.0.2",
    "@ngrx/entity": "^18.0.2",
    "@ngrx/store": "^18.0.2",
    "@ngrx/store-devtools": "^18.0.2",
    "connect-history-api-fallback": "^2.0.0",
    "express": "^4.21.0",
    "primeng": "^17.18.10",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^10.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/cli": "^18.2.5",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/localize": "^18.2.6",
    "@chromatic-com/storybook": "^1.9.0",
    "@compodoc/compodoc": "^1.1.25",
    "@storybook/addon-docs": "^8.3.3",
    "@storybook/addon-essentials": "^8.3.3",
    "@storybook/addon-interactions": "^8.3.3",
    "@storybook/addon-links": "^8.3.3",
    "@storybook/addon-onboarding": "^8.3.3",
    "@storybook/angular": "^8.3.3",
    "@storybook/blocks": "^8.3.3",
    "@storybook/test": "^8.3.3",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^10.0.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "storybook": "^8.3.3",
    "typescript": "~5.5.2"
  }
